{
   "manifest_version": 2,
   "name": "FT Suggest",
   "description": "Suggests",
   "version": "0.0.1",
   "icons": {
      "128": "ft.png"
   },
   "author": "",
   "browser_action": {
      "default_icon": "ft.png"
   },
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "css": ["css/main.css"],
         "js": ["js/main.js"],
         "run_at": "document_idle"
      }
   ],
   "permissions": [
      "activeTab"
   ]
}
